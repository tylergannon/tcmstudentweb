.child
  %p
    %b Signs and Symptoms
    != tfwac f, :name, 'patterns'
    != text_area_with_auto_complete f, :pattern_symptoms_text, 'symptoms', :rows => 6, :cols => 60
    %b!= "Tags / Disease / Primary Pattern #{tag_field(f, :tag_list)}#{tag_field(f, :disease_list)}#{tag_field(f, :primary_pattern_list)}"

  %p
    = f.label :commentary
    %br
    = f.text_area :commentary, :rows => 2, :cols => 40
  %p
    = render :partial => 'citations/edit_citation', :locals => {:f => f, :field => :citation}


  %p
    = remove_child_link 'remove', f

