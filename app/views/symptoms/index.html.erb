<% title "Symptoms" %>

<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for symptom in @symptoms %>
    <tr>
      <td><%=h symptom.name %></td>
      <td><%= link_to "Show", symptom %></td>
      <td><%= link_to "Edit", edit_symptom_path(symptom) %></td>
      <td><%= link_to "Destroy", symptom, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Symptom", new_symptom_path %></p>
