- title "Pulse Qualities"
%table
  %tr
    %th
      Name
    %th
      Commentary
  - for pulse_quality in @pulse_qualities
    %tr
      %td
        = h pulse_quality.name
      %td
        = h pulse_quality.commentary
      %td
        = link_to "Show", pulse_quality
      %td
        = link_to "Edit", edit_pulse_quality_path(pulse_quality)
      %td
        = link_to "Destroy", pulse_quality, :confirm => 'Are you sure?', :method => :delete
%p
  = link_to "New Pulse Quality", new_pulse_quality_path
